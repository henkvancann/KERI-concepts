{
    "specs": [
        {
            "title": "Spec-Up-T Starterpack Example",
            "spec_directory": "./spec",
            "spec_terms_directory": "terms-definitions",
            "output_path": "./docs",
            "markdown_paths": [
                "terms-definitions/term-1.md",
                "terms-definitions/term-2.md",
                "terms-definitions/internet-scale-digital-trust.md",
                "terms-definitions/repair-the-internet.md",
                "concept.md"
            ],
            "logo": "https://raw.githubusercontent.com/trustoverip/logo-assets/master/logos/ToIP-Logo-Color-SolidDimensional-Horizontal-LightOnDark.svg",
            "logo_link": "https://github.com/trustoverip/specification-template",
            "katex": false,
            "source": {
                "host": "github",
                "account": "blockchainbird",
                "repo": "spec-up-t-starter-pack"
            },
            "external_specs": [
                {
                    "PE": "https://identity.foundation/presentation-exchange"
                },
                {
                    "test-1": "https://kordwarshuis.github.io/spec-up-xref-test-1/"
                },
                {
                    "test-2": "https://kordwarshuis.github.io/spec-up-xref-test-2/"
                }
            ],
            "external_specs_repos": [
                {
                    "external_spec": "PE",
                    "url": "https://github.com/decentralized-identity/presentation-exchange",
                    "terms_dir": "spec"
                },
                {
                    "external_spec": "test-1",
                    "url": "https://github.com/kordwarshuis/spec-up-xref-test-1",
                    "terms_dir": "spec/term-definitions"
                },
                {
                    "external_spec": "test-2",
                    "url": "https://github.com/kordwarshuis/spec-up-xref-test-2",
                    "terms_dir": "spec/term-definitions"
                }
            ],
            "assets": [
                {
                    "path": "/custom-assets/custom.css"
                },
                {
                    "path": "/custom-assets/custom-head.js",
                    "inject": "head"
                },
                {
                    "path": "/custom-assets/custom-body.js",
                    "inject": "body",
                    "module": true
                }
            ],
            "katex": false,
            "searchHighlightStyle": "ssi"
        }
    ]
}